{
    "name": "digbang/money",
    "description": "Money PHP bridge for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Darío Govergun",
            "email": "dgovergun@digbang.com"
        },
        {
            "name": "Juan Falcón",
            "email": "jfalcon@digbang.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "moneyphp/money": "^3.0",
        "laravel/framework": "^7.0|^8.0",
        "laravel-doctrine/fluent": "^1.1",
        "laravel-doctrine/orm": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Digbang\\Money\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Digbang\\Money\\Tests\\": "tests/"
        }
    }
}
